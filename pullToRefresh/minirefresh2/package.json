{
  "name": "minirefresh",
  "title": "minirefresh",
  "version": "1.0.8",
  "description": "优雅的H5下拉刷新。零依赖，高性能，多主题，易拓展。 ",
  "homepage": "https://github.com/minirefresh/minirefresh",
  "repository": {
    "type": "git",
    "url": "https://github.com/minirefresh/minirefresh.git"
  },
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/minirefresh/minirefresh/issues"
  },
  "keywords": [
    "minirefresh",
    "pulltorefresh",
    "pulldownrefresh",
    "pulluprefresh",
    "pull-refresh",
    "refresh"
  ],
  "author": "dailc(dailichun@aliyun.com)",
  "license": "MIT",
  "scripts": {
    "build": "node_modules/.bin/gulp default",
    "watch": "node_modules/.bin/gulp watch",
    "dev": "npm run build && npm run watch",
    "test": "node_modules/.bin/karma start test/unit/karma.cover.config.js",
    "test:sauce": "node_modules/.bin/karma start test/unit/karma.sauce.config.js",
    "test:cover": "node_modules/.bin/karma start test/unit/karma.cover.config.js",
    "test:unit": "node_modules/.bin/karma start test/unit/karma.unit.config.js",
    "test:eslint": "npm run eslint:report",
    "eslint": "node_modules/.bin/eslint ./src ./test",
    "eslint:fix": "node_modules/.bin/eslint ./src ./test --fix",
    "eslint:report": "node_modules/.bin/eslint ./src ./test -f html -o ./_report/eslint.html"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.10",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.1",
    "codecov": "^2.3.0",
    "eslint": "^4.5.0",
    "gulp-eslint": "^4.0.0",
    "gulp-header": "^1.8.8",
    "gulp-sequence": "^0.4.6",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.5",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {}
}
